{#  ## args: (string [array]) #}
{% if EphemeralContainer.args is defined %}
args:
{%   for arg in EphemeralContainer.args | default([], true) %}
  - "{{ arg | default('') }}"
{%   endfor %}
{% endif -%}

{#  ## command: (string [array]) #}
{% if EphemeralContainer.command is defined %}
command:
{%   for cmd in EphemeralContainer.command | default([], true) %}
  - "{{ cmd | default('') }}"
{%   endfor %}
{% endif -%}

{#  ## env: (EnvVar [array]) #}
{% if EphemeralContainer.env is defined %}
env:
{%   for EnvVar in EphemeralContainer.env | default([], true) %}
{%     filter indent(width=4, first=false) %}
  - {%   include '_EnvVar.j2' %}
{%     endfilter %}
{%   endfor %}
{% endif -%}

{#  ## envFrom: (EnvFromSource [array]) #}
{% if EphemeralContainer.envFrom is defined %}
envFrom:
