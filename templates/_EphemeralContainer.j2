{%     endfilter %}
{%   endfor %}
{% endif -%}

{#  ## image (string) #}
{% if EphemeralContainer.image is defined %}
image: {{ EphemeralContainer.image | default('') }}
{% endif -%}

{#  ## imagePullPolicy (string) #}
{% if EphemeralContainer.imagePullPolicy is defined %}
imagePullPolicy: {{ EphemeralContainer.imagePullPolicy | default('') }}
